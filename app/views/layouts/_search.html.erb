<%=render 'layouts/simple_second_nav' %>

<div class="container" style="padding-top:84px;">
  <%= form_tag(yield(:forwardPath), method: :get) do %>
    <div class="form-group">
      <%= label_tag(:expression, "Search for:") %>
      <%= text_field_tag :expression, '', class:'form-control' %>
    </div>
    <%= submit_tag "Search", class: "btn btn-primary btn-md" %>
  <% end %>
</div>
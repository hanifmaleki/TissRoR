<h1>Project#detail</h1>
<p>Find me in app/views/project/detail.html.erb</p>

<%=render 'layouts/detail' %>
<% project.each do |key, value| %>
  <h4><%= key %>:</h4>
  <% if value.is_a?(Hash) %>
    <% value.each do |k, v| %>
      <h5><%= k %>:</h5>
      <%= raw(v).html_safe %>
      <br>
    <% end %>
  <% else %>
    <%= raw(value).html_safe %>
  <% end %>
  <br>
<% end %>
<%=link_to "Back to list", person_doInvoke_path %>
